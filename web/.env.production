# Archivo de ejemplo para configuración del Web en la VM
# Copiar a: /var/www/inmube/web/.env.production
# y reemplazar los valores según tu configuración

# URL de la API (Admin Panel)
NEXT_PUBLIC_API_URL="https://admin.inmube.com"

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY="tu_firebase_api_key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="tu_firebase_auth_domain"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="tu_firebase_project_id"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="tu_firebase_storage_bucket"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="tu_firebase_messaging_sender_id"
NEXT_PUBLIC_FIREBASE_APP_ID="tu_firebase_app_id"

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="tu_google_maps_key"

# Google Analytics (opcional)
NEXT_PUBLIC_GA_ID=""

# Sitio y configuración general
NEXT_PUBLIC_SITE_URL="https://inmube.com"
NEXT_PUBLIC_SITE_NAME="Inmube"

# Configuración de puerto (Node.js)
PORT=3000
NODE_ENV="production"

# Compresión
COMPRESS=true
